{
    "name": "mahindra",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@sap-cloud-sdk/http-client": "^3.7.0",
        "@sap-cloud-sdk/resilience": "^3.7.0",
        "@sap/cds": "^7",
        "@sap/cds-hana": "^2",
        "@sap/xssec": "^3.3.5",
        "express": "^4",
        "passport": "^0.6.0",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@cap-js/sqlite": "^1",
        "@sap/cds-dk": "^7",
        "@sap/ux-specification": "^1.120.8",
        "rimraf": "^3.0.2"
    },
    "scripts": {
        "start": "cds-serve",
        "watch-vob_mah": "cds watch --open vob_mah/webapp/index.html?sap-ui-xx-viewCache=false",
        "undeploy": "cf undeploy mahindra --delete-services --delete-service-keys --delete-service-brokers",
        "build": "rimraf resources mta_archives && mbt build --mtar archive",
        "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
        "watch-vob_screen2": "cds watch --open vob_screen2/webapp/index.html?sap-ui-xx-viewCache=false",
        "watch-vobscreen3": "cds watch --open vobscreen3/webapp/index.html?sap-ui-xx-viewCache=false",
        "watch-vobscreen4": "cds watch --open vobscreen4/webapp/index.html?sap-ui-xx-viewCache=false",
        "watch-vobscreen4b": "cds watch --open vobscreen4b/webapp/index.html?sap-ui-xx-viewCache=false",
        "watch-vobscreen4bb": "cds watch --open vobscreen4bb/webapp/index.html?sap-ui-xx-viewCache=false",
        "watch-finalwizard": "cds watch --open finalwizard/webapp/index.html?sap-ui-xx-viewCache=false",
        "watch-folderstructure": "cds watch --open folderstructure/webapp/index.html?sap-ui-xx-viewCache=false",
        "watch-folderstruc": "cds watch --open folderstruc/webapp/index.html?sap-ui-xx-viewCache=false"
    },
    "cds": {
        "requires": {
            "auth": "dummy",
            "destinations":true,
            "BPA_trigger": {
                "kind": "odata-v2",
                "csrf": true,
                "csrfInBatch": true,
                "credentials": {
                    "destination": "sap_bpa_trigger",
                    "requestTimeout": 120000
                },
                "destinationOptions": {
                    "selectionStrategy": "alwaysProvider",
                    "jwt": null
                }
            }
        }
    },
    "sapux": [
        "app/vob_mah",
        "app/vob_screen2",
        "app/vobscreen3",
        "app/vobscreen4",
        "app/vobscreen4b",
        "app/vobscreen4bb"
    ]
}