<mvc:View xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m" controllerName="finalwizard.controller.ObjectPage" displayBlock="true"
    xmlns:m="sap.m"
    xmlns:u="sap.uxap"
    xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    	xmlns:upload="sap.m.upload">
    <NavContainer id="wizardNavContainer" height="120vh">
        <pages>
            <Page id="wizardContentPage" showHeader="false">
                <content>
                    <Wizard id="CreateProductWizard" class="height1" renderMode="Page">
                        <WizardStep id="partrequest" class="wizardStepClass" title="VOB-Part Request" validated="true" icon ="sap-icon://request">
                        <!-- <Text text="sssss"></Text> -->
                           <core:ComponentContainer id="screen1" propagateModel="true" height="75%" />
                        </WizardStep>
                        <WizardStep id="supplierdetails" class="wizardStepClass" validated="true" title="VOB-Supplier Details" activate="screen2activate" icon ="sap-icon://supplier">
                            <core:ComponentContainer id="screen2" propagateModel="true" height="75%" />
                        </WizardStep>
                        <WizardStep id="OptionalInfoStep" class="wizardStepClass" validated="true" activate="screen3activate" title="VOB-Document Upload" icon="sap-icon://add-document">
                            <core:ComponentContainer id="screen3" propagateModel="true" height="75%" />
                        </WizardStep>
                        <WizardStep id="vobapproval1" class="wizardStepClass" complete="pricingComplete" validated="true" title="VOB-Approval" activate="screen4activate" icon="sap-icon://employee-approvals">
                            <core:ComponentContainer id="screen4" propagateModel="true" height="75%" />
                        </WizardStep>
                    </Wizard>
                </content>
                <!-- <footer>
                    <OverflowToolbar id="oflow1">
                        <Button id="sss" text="Cancel" press="handleWizardCancel" />
                    </OverflowToolbar>
                </footer> -->
            </Page>
        </pages>
    </NavContainer>
</mvc:View>
