<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:macros="sap.fe.macros"
>
    <HBox id="mainHBox">
        <VBox>
            <Table
                id="parentTable"
                class="tableClass"
            >
                <columns>
                    <Column>
                        <Label text="MGSP Part No" />
                    </Column>
                    <Column>
                        <Label text="Existing MGSP PO Price" />
                    </Column>
                    <Column>
                        <Label text="Target Price" />
                    </Column>
                </columns>
            </Table>
            <VBox
                alignItems='End'
                class="vboxbelowtable"
            >
                <Label text="Total Landed Investment - Settled (Rs. lacs)" />
                <Label text="Total Business value (Rs. Lac) 1st 12months" />
                <Label
                    text="Amount to be paid to M/s RKR (Lac) 1st 12 months"
                />
                <Label
                    text="Amount to be paid to M/s RKR (Lac) 2nd + 3rd Year"
                />
                <Label text="YOY Reduction for 3 yrs (After SOP + 1 year)" />
                <Label text="FX Base Rate" />
                <Label text="Finished Weight of Part / System (kg)" />
                <Label text="DTB Packaging Cost Rs" />
                <Label text="Inco Terms" />
                <Label text="Payment Terms" />
                <Label text="Transport Cost" />

                <HBox class="innerhbox">
                    <Label
                        class="hboxlabel1"
                        text="Capex"
                    />
                    <VBox class="innervbox">
                        <Label text="Tooling / Dies / Moulds / Fixtures" />
                        <Label text="Inspection Gauges" />
                    </VBox>
                </HBox>

                <HBox class="innerhbox">
                    <Label
                        class="hboxlabel2"
                        text="Revenue"
                    />
                    <VBox class="innervbox">
                        <Label text="Testing / Validation" />
                        <Label text="Engg Fees" />
                        <Label text="Proto Tooling" />
                        <Label text="Logistics Trollies" />
                    </VBox>
                </HBox>
                <Label text="Total Landed Investment Settled" />
            </VBox>
        </VBox>
        <HBox class="VendorHBoxClass" />
    </HBox>
</core:FragmentDefinition>
