<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:macros="sap.fe.macros"
displayBlock="true"
xmlns:mvc="sap.ui.core.mvc"
	xmlns:upload="sap.m.upload">
	<VBox id= "11" >
	<Button id="dewdewdw" visible="false" core:require="{ handler: 'vobscreen4/ext/fragment/Attachments'}" text="Jhggfds" press="handler.onPress" />
						<upload:UploadSet
					
					id="uploadSet"
					core:require="{ handler: 'vobscreen4/ext/fragment/Attachments'}"
					instantUpload="false"
					uploadEnabled="true"
					showIcons="true"
					afterItemAdded="handler.onAfterItemAdded"
					uploadCompleted="handler.onUploadCompleted"
					items="{
								path: '/Files',
								parameters: {
									$orderby: 'createdAt desc'
								},
								templateShareable: false}"
				>
					<upload:toolbar>
					</upload:toolbar>
					<upload:items>
						<upload:UploadSetItem 
						id="ddd"
							fileName="{fileName}"
							mediaType="{mediaType}"
							url="{url}"
							thumbnailUrl="{
								path: 'mediaType',
								formatter: 'handler.formatThumbnailUrl'
							}"
							enabledEdit="true"
							openPressed="handler.onOpenPressed"
							removePressed="handler.onRemovePressed"
							mode="None"
							
						>
							<upload:attributes>
								<ObjectAttribute
								id="dd"
									title="Uploaded By"
									text="{createdBy}"
									active="false"
									
								/>
								<ObjectAttribute
								id="dd22"
									title="Uploaded on"
									text="{createdAt}"
									active="false"
								/>
								<ObjectAttribute
								id="dddw2"

									title="File Type"
									text="{mediaType}"
									active="false"
								/>
								<ObjectAttribute
								id="dd22a"
									title="Folder"
									text="{Folder}"
									active="false"
								/>
							</upload:attributes>
						</upload:UploadSetItem>
					</upload:items>
				</upload:UploadSet>
	</VBox>
</core:FragmentDefinition>